<launch>
    <node name="turtlesim_node" pkg="turtlesim" type="turtlesim_node" />
    <node name="turtle1" pkg="turtlesim" type="turtle_teleop_key" output="screen">
        <remap from="turtle1/cmd_vel" to="turtle1/cmd_vel"/>
    </node>
    <node name="turtle2" pkg="turtlesim" type="turtle_teleop_key" output="screen">
        <remap from="turtle1/cmd_vel" to="turtle2/cmd_vel"/>
    </node>
    <node name="turtle3" pkg="turtlesim" type="turtle_teleop_key" output="screen">
        <remap from="turtle1/cmd_vel" to="turtle3/cmd_vel"/>
    </node>
    <node name="random_motion_with_wall_avoidance" pkg="robot_pkg" type="gptautocolli6.py" output="screen"/>
</launch>

